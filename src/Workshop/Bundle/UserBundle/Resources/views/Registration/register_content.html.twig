{% trans_default_domain 'FOSUserBundle' %}
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_row(form.email) }}

        {{ form_row(form.oxagileSalary) }}
    {{ price(form.oxagileSalary.vars.value) }}

    {{ form_row(form.pastProject1) }}

    {{ form_row(form.pastProject2) }}

    {{ form_row(form.pastProject3) }}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans }}" />
    </div>
    {{ form_end(form) }}

</form>
